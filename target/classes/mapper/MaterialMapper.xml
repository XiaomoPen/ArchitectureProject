<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.seven.springboot.mapper.pht.MaterialDao">
<!--    批量新增材料明细表-->
    <insert id="addMaterialDetails" parameterType="java.util.List">
        insert into t_pj_materialPlanDetailed values 
        <foreach collection="list" item="item" index="index" open=" " separator="," close=" ">
            (#{item.planNumber},#{item.materialName},#{item.materialNumber},#{item.materialCompany},#{item.materialSum},#{item.materialConsultMoney},#{item.materialMoney},#{item.materialMask})
        </foreach>
    </insert>
</mapper>